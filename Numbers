num1 = 25
num2 = float(num1)
print(type(num2))

num1 = 13.67
num2 = int(num1)
print(type(num2))

num1 = 10
num2 = 3

num3 = num1 ** num2
print(num3 == 250 * 4)

num1 = 5
num2 = 2
num3 = int(num1/num2)
print(num3 == 5 % 3) #result is True

num1 = -11
num2 = -1 * num1
#or num2 = abs(num1)
print(num2 == 11)
